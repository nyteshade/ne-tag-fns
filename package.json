{
  "name": "ne-tag-fns",
  "version": "0.6.9",
  "description": "A small template tag function library for ES6 string literals",
  "main": "dist/functions.js",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist/* ; babel -s --source-root src src/ -d dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyteshade/ne-tag-fns.git"
  },
  "keywords": [
    "es6 strings",
    "template strings",
    "tag",
    "functions"
  ],
  "author": "Brielle Harrison",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nyteshade/ne-tag-fns/issues"
  },
  "homepage": "https://github.com/nyteshade/ne-tag-fns#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-flow": "^7.0.0",
    "babel-7-jest": "^21.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "flow-bin": "^0.121.0",
    "jest": "^23.6.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ],
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-transform-flow-strip-types"
    ],
    "sourceMaps": "both",
    "retainLines": true
  },
  "dependencies": {}
}
