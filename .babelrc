{
  "presets": [
    ["env", {
      "targets": { "node": "6" },
      "modules": "commonjs",
      "useBuiltIns": true,
      "debug": false
    }],
    "stage-1",
    "flow"
  ],
  "plugins": [
    ["transform-class-properties", { "spec": true }],
    "transform-runtime",
    "transform-decorators-legacy"
  ],
  "sourceMaps": "both"
}
